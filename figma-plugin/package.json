{
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.3",
    "@create-figma-plugin/utilities": "^4.0.3",
    "preact": ">=10",
    "tailwindcss": "^3.4.18"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@figma/plugin-typings": "1.100.2",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "concurrently": ">=8",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": ">=4"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run build:css && cross-env NODE_ENV=development npm run build:js",
    "build:prod": "npm run build:css && cross-env NODE_ENV=production npm run build:js",
    "build:css": "tailwindcss --input ./src/input.css --output ./src/output.css",
    "build:js": "build-figma-plugin --typecheck --minify",
    "watch": "npm run build:css && concurrently npm:watch:css npm:watch:js",
    "watch:css": "tailwindcss --input ./src/input.css --output ./src/output.css --watch",
    "watch:js": "cross-env NODE_ENV=development build-figma-plugin --typecheck --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "check": "npm run format:check && npm run lint && npm run type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "figma-ui-reviewer",
    "name": "Figma UI Reviewer",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
